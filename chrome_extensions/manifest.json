{	
	"name": "ckQRCode",
	"description": "test replace QRCode",
	"manifest_version": 2,
	"version": "1.0",
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["bundle.js"],
		"run_at": "document_idle"
	}],
"web_accessible_resources": [
    "dist/bundle.js"
  ],
	"permissions": ["tabs","<all_urls>"]
}
